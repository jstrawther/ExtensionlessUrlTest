			</staticContent>
		</system.webServer>
	</location>
	<location path="assetlinks.json">
		<system.webServer>
			<staticContent>
				<remove fileExtension=".json" />
				<mimeMap fileExtension=".json" mimeType="application/json" />
				<clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="08:00:00" cacheControlCustom="public" />
